<!DOCTYPE html>

<html lang="en">
  
<%- include('layouts/headers') -%>


<body class="bg-white">
    
    <%- include('layouts/navbar') -%>
    
    <%- include ('ejsFunction') %> <!--function javascript ejs-->
    
<main class="container">
  
  <div class="d-flex justify-content-between m-3">
            
            <a class="text-primary small" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Goto view fee" href="/stdList"><i class="fas fa-arrow-left text-dark fw-bold"></i> Go Back</i> </a>
            
            </div>
    
    <div class="text-center mt-4">

         <p class="text-secondary fw-normal"><%= viewTitle %> : <span class="text-danger"><%= moment().format('DD/MM/YYYY'); %> <%= moment().format("[(]dddd[)]"); %></span></p>
         
    </div>
    
        <!--Today fee review table started-->
   
           <ul class="list-group"> 
        
              <div class="table-responsive">
        
              <table class="table border bg-white border-start border-end shadow-sm">
                
                   <tbody>
                     
                    <tr class="text-center">
                        
                      <th class="fw-bold bgColor" colspan="6"> Today fee received</th>
                      
                    </tr>
                    
                    <tr class="bg-light">
                      
                      <td class="fw-normal border align-middle small"> Regn.</td>
                      
                      <td class="fw-normal border align-middle small"> Student's Name</td>
                      
                      <td class="fw-normal border align-middle small"> Father's Name </td>
                      
                      <td class="fw-normal border align-middle small"> Narration </td>
      
                      <td class="fw-normal border align-middle small"> Amount </td>
                      
                      <td class="fw-normal border align-middle small"> Entered By </td>
      
                    </tr>
      <% 
             for (var i=0; i < students.length; i++) {
              
                    if( students[i].studentFee != undefined ) {
          
                    for (var k=0; k < students[i].studentFee.length; k++) {
                        
                        if( students[i].studentFee[k] != undefined &&  moment(students[i].studentFee[k].dateSubmitted).format('YYYY-MM-DD') ==  moment().format('YYYY-MM-DD')) {

              %>
                        <tr>
                          
                          <td class="text-center border" ><%= students[i].regn %></td>
                          
                          <td class="border" >
                            
                            <a href="/computer/<%= students[i]._id %>">
          
                                <%= capitalizeFirstLetter(students[i].username) %>
                                
                            </a>
                            
                          </td>
                          
                          <td class="border"> <%= students[i].fname %> </td>
                          
                          <td class="border fst-italic"> <%= students[i].studentFee[k].narrationInput %> </td>
                          
                          <td class="border">Rs.  <%= students[i].studentFee[k].feeAmount %> </td>
                      
                          <td class="border">  <%= capitalizeFirstLetter(students[i].studentFee[k].verifierId.username) %> </td>
        
                        </tr>
                        
                        <% } } } } %>
                            
                      </tbody>
                      
                    </table>
                    
                    </div>
            
            </ul> <!--today fee review table end-->
            
            <hr class="text-success  w-25 mx-auto mb-4">
            <!--Today exam fee review table started-->
   
           <ul class="list-group"> 
        
              <div class="table-responsive">
        
              <table class="table border bg-white border-start border-end shadow-sm">
                
                   <tbody>
                     
                    <tr class="text-center">
                        
                      <th class="fw-bold bgColor" colspan="6"> December 2021 Batch</th>
                      
                    </tr>
                    
                    <tr class="bg-light">
                      
                      <td class="fw-normal border align-middle small"> Regn.</td>
                      
                      <td class="fw-normal border align-middle small"> Student's Name</td>
                      
                      <td class="fw-normal border align-middle small"> Father's Name </td>
                      
                      <td class="fw-normal border align-middle small"> Narration </td>
      
                      <td class="fw-normal border align-middle small"> Amount </td>
                      
                      <td class="fw-normal border align-middle small"> Entered By </td>
      
                    </tr>
              <% 
              
             for (var i=0; i < students.length; i++) {
              
                    if( students[i].studentExamFee != undefined ) {
          
                    for (var k=0; k < students[i].studentExamFee.length; k++) {
                        
                        if( students[i].studentExamFee[k] != undefined &&  moment(students[i].studentExamFee[k].dateSubmitted).format('YYYY-MM-DD') ==  moment().format('YYYY-MM-DD')) {
              
              %>
              
                        <tr>
                          
                          <td class="text-center border" ><%= students[i].regn %></td>
                          
                          <td class="border" >
                            
                            <a href="/computer/<%= students[i]._id %>">
          
                                <%= capitalizeFirstLetter(students[i].username) %>
                                
                            </a>
                            
                          </td>
                          
                          <td class="border"> <%= students[i].fname %> </td>
                          
                          <td class="border fst-italic"> <%= students[i].studentExamFee[k].narrationInput %> </td>
                          
                          <td class="border">Rs.  <%= students[i].studentExamFee[k].feeAmount %> </td>
                      
                          <td class="border">  <%= capitalizeFirstLetter(students[i].studentExamFee[k].verifierId.username) %> </td>
        
                        </tr>
                        
                        <% } } } } %>
                        
                      </tbody>
                      
                    </table>
                    
                    </div>
            
            </ul> <!--today exam fee review table end-->
            
            <hr class="text-success w-25 mx-auto mb-4">
            
            <!--Today other fee review table started-->
   
           <ul class="list-group"> 
        
              <div class="table-responsive">
        
              <table class="table border bg-white border-start border-end shadow-sm">
                
                   <tbody>
                     
                    <tr class="text-center">
                        
                      <th class="fw-bold bgColor" colspan="6"> June 2022 Batch</th>
                      
                    </tr>
                    
                    <tr class="bg-light">
                      
                      <td class="fw-normal border align-middle small"> Regn.</td>
                      
                      <td class="fw-normal border align-middle small"> Student's Name</td>
                      
                      <td class="fw-normal border align-middle small"> Father's Name </td>
                      
                      <td class="fw-normal border align-middle small"> Narration </td>
      
                      <td class="fw-normal border align-middle small"> Amount </td>
                      
                      <td class="fw-normal border align-middle small"> Entered By </td>
      
                    </tr>
              <% 
              
             for (var i=0; i < students.length; i++) {
              
                    if( students[i].studentOtherFee != undefined ) {
          
                    for (var k=0; k < students[i].studentOtherFee.length; k++) {
                        
                        if( students[i].studentOtherFee[k] != undefined &&  moment(students[i].studentOtherFee[k].dateSubmitted).format('YYYY-MM-DD') ==  moment().format('YYYY-MM-DD')) {
                            
              %>
              
                        <tr>
                          
                          <td class="text-center border" ><%= students[i].regn %></td>
                          
                          
                          
                          <td class="border" >
                            
                            <a href="/computer/<%= students[i]._id %>">
          
                                <%= capitalizeFirstLetter(students[i].username) %>
                                
                            </a>
                            
                          </td>
                          
                          <td class="border"> <%= students[i].fname %> </td>
                          
                          <td class="border fst-italic"> <%= students[i].studentOtherFee[k].narrationInput %> </td>
                          
                          <td class="border">Rs.  <%= students[i].studentOtherFee[k].feeAmount %> </td>
                      
                          <td class="border">  <%= capitalizeFirstLetter(students[i].studentOtherFee[k].verifierId.username) %> </td>
        
                        </tr>
                        
                        <% } } } } %>
                            
                      </tbody>
                      
                    </table>
                    
                    </div>
            
            </ul> <!--today other fee review table end-->

</main>
    
    <%- include('layouts/scripts') -%>
    
    <%- include('layouts/footer') -%>
    


  </body>
  
</html>
