                   <%
  // Example: you might have a flag to tell if this is the "current" student
  // (e.g. from the database or session)
  const isCurrentStudent = studentFee.some(f => f.dateofpayment >= "2024-12-31") ||
  studentExamFee.some(f => f.dateofpayment >= "2024-12-31") ||
  studentOtherFee.some(f => f.dateofpayment >= "2024-12-31" ) ; 

  let isPaid = false;

  if (isCurrentStudent) {
    // Current student rule: must pay full 1670
    isPaid =
      (Array.isArray(studentFee) && studentFee.some(f => f.feeAmount >= 1670)) ||
      (Array.isArray(studentExamFee) && studentExamFee.some(f => f.feeAmount == 1670)) ||
      (Array.isArray(studentOtherFee) && studentOtherFee.some(f => f.feeAmount >= 1670));
  } else {
    // Previous student rule: 499 or more is enough
    isPaid =
      (Array.isArray(studentFee) && studentFee.some(f => f.feeAmount >= 499)) ||
      (Array.isArray(studentExamFee) && studentExamFee.some(f => f.feeAmount >= 499)) ||
      (Array.isArray(studentOtherFee) && studentOtherFee.some(f => f.feeAmount >= 499));
  }
%>

